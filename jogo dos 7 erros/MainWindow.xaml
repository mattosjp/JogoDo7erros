        <TextBox HorizontalAlignment="Left" Margin="934,147,0,0" TextWrapping="Wrap" Text="Imagem errada" VerticalAlignment="Top" Width="370"  SelectionBrush="#FF70168C" BorderBrush="#FF70168C" Background="#FF70168C" Foreground="#FFF9B700" FontSize="48" Height="170" Grid.RowSpan="2" Grid.Row="1"/>
        <Image x:Name="imgPato" HorizontalAlignment="Left" Height="166" Margin="1408,141,0,0" Grid.RowSpan="2" Grid.Row="1" VerticalAlignment="Top" Width="284" Source="/Educk img.png" MouseDown="ClickPato"/>

