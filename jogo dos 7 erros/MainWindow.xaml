        <TextBlock x:Name="click4" HorizontalAlignment="Left" Margin="1247,202,0,0" TextWrapping="Wrap" VerticalAlignment="Top" MouseDown="Click_4" RenderTransformOrigin="0.5,0.5" Height="20" Width="23">
            <TextBlock.RenderTransform>
                <TransformGroup>
